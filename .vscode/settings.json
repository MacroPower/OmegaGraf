{
  "cSpell.words": [
    "Figgle",
    "Jsonnet",
    "blinky",
    "flurl",
    "graffonetlibs",
    "hsep",
    "ipam",
    "nett",
    "npipe",
    "omnisharp",
    "owin",
    "scroller",
    "tsdb",
    "unmount",
    "vcenter",
    "vcsim"
